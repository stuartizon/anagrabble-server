[
  {
    "name": "anagrabble-server",
    "image": "stuartizon/anagrabble-server:${application_version}",
    "essential": true,
    "portMappings": [
      {
        "containerPort": 8080
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "anagrabble-server",
        "awslogs-region": "ap-southeast-2",
        "awslogs-stream-prefix": "anagrabble-server"
      }
    }
  }
]